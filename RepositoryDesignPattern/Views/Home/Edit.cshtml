@model IEnumerable<RepositoryDesignPattern.Models.EmployeeViewModel>
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>


    @foreach (var item in Model)
    {
        <div class="form-horizontal">
            <h4>Student</h4>
            <hr/>
       
        </div>
            <div class="EmployeeID" id="EmployeeID">
                @Html.LabelFor(model => item.EmployeeID, htmlAttributes: new { @class = "control-label col-md-2" })
