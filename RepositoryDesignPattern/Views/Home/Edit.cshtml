                    @Html.EditorFor(model => item.Gender, new { htmlAttributes = new { @class = "form-control",@id="gender" } })
                </div>
            </div>
            <div class="Salary" id="Salary">
                @Html.LabelFor(model => item.Salary, htmlAttributes: new { @class = "control-label col-md-2" })
